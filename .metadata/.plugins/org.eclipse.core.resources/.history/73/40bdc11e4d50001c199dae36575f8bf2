package AcalismaALani;
 class insan{
	 int boy;
	 int kilo;
	 int yas;
	 public insan() {
		 System.out.println("bir isnan objesi olusturuldiu");
		 yas = 1;
	 }
	 public insan(int yas , int kilo,int boy)
 }

public class ConstructorYazilimBilimi {

	public static void main(String[] args) {
		insan veli=new insan(12,22,150);
		insan ali=new insan();
		ali.boy=180;
		ali.kilo=80;
		

	}

}
