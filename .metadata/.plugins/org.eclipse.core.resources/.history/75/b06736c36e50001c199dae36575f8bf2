package Batch30Calisma.day25_constructor;

public class C01 {

	public static  void main(String[] args) {
		
		public int ilanNo;  //burada deger ataamimza gerekyok cunku class leveldir ve default degerler gecerlidir.
		public String marka;
		public String model;
		public int yil;
		public int fiyat;
		
		public void maxHiz(int hiz) {
			System.out.println("Araba" + hiz +" km hiz yapabilir");
		}
		 
		public void yakit (String yakitTuru) {
			System.out.println("Araba yakit olrak" + yakitTuru +"kullanir");
		}
}

	


