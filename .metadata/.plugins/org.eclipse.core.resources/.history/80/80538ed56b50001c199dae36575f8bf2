package AcalismaALani;

import java.util.ArrayList;

public class ConstructorOrnek {

	public static void main(String[] args) {
	ArrayList<Character>list=new ArrayList<Character>();
	for(char i='a';i<='e';i++) {
		list.clear();
		list.add(i);
		
	}
	System.out.println(list);

	}

}
//once doner sonra clear siler tekar basa doner clear siler add ekler clear siler add ekler clear siler en son e gelir.