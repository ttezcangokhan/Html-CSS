package AcalismaALani;

import java.util.ArrayList;
import java.util.List;

public class ConstructorSulogil {

	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		list.add("ali");
		list.add("veli");
		list.add("gokhan");
		list.set(1,"Havva");
		System.out.println(list);
}
}
